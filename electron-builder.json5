/**
 * @see https://www.electron.build/configuration/configuration
 */
{
    appId: 'YourAppID',
    directories: {
        output: 'release/${version}',
    },
    files: ['dist'],
    linux: {
        target: [
            {
                target: 'dir',
                arch: ['x64'],
            },
        ],
        artifactName: '${productName}_${version}.${ext}',
    },
    mac: {
        artifactName: '${productName}_${version}.${ext}',
        target: ['dmg'],
    },
    win: {
        target: [
            {
                target: 'nsis',
                arch: ['x64'],
            },
        ],
        artifactName: '${productName}_${version}.${ext}',
    },
    nsis: {
        oneClick: false,
        perMachine: false,
        allowToChangeInstallationDirectory: true,
        deleteAppDataOnUninstall: false,
    },
}
